{
  "name": "todo-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "prepare": "husky install",
    "test": "react-scripts test --watchAll=false --env=jsdom --onlyChanged --bail",
    "test:watch": "react-scripts test --coverage --watch --maxWorkers=25%",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test start 3000 cy:open",
    "test:e2e:ci": "start-server-and-test start 3000 cy:run",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "npm test"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "react-spring": "8.0.27",
    "styled-components": "^5.2.3",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@jackfranklin/test-data-bot": "^1.4.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.3",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "axios-mock-adapter": "^1.20.0",
    "cypress": "^9.5.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^5.1.3",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^10.5.4",
    "prettier": "2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.4.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
